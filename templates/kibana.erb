server {
        listen  80;
        listen  [::]:80;
        server_name _;

        access_log  /var/log/nginx/kibana-80.access.log;


        location / {
                proxy_pass      http://127.0.0.1:5601/;
                include         /etc/nginx/proxy_params;
                auth_basic "Restricted";
                auth_basic_user_file /etc/nginx/htpasswd/kibana;
        }
}
